{
  "name": "sakana-widget-react",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@8.12.1",
  "description": "A React component that wrap \"Sakana! Widget\".",
  "author": "pinkchampagne",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.tsx --clean --dts --format=esm,cjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "biome format .",
    "format:fix": "pnpm run format --write",
    "lint": "env ESLINT_USE_FLAT_CONFIG=true eslint . --cache",
    "lint:fix": "pnpm run lint --fix"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sakana-widget": "^2.7.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@pinkchampagne/tsconfig": "^1.0.0",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/test": "^7.6.7",
    "@swc/core": "^1.3.102",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-pcp": "2.0.0-beta.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "prop-types": "^15.8.1",
    "storybook": "^7.6.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
